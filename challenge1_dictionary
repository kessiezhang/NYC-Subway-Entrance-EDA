    "import pprint\n",
    "mta_dict1 = {}\n",
    "for sublist in lst:\n",
    "    key = tuple(sublist[:4])\n",
    "    value = [text.strip(' ') for text in sublist[4:]]\n",
    "    if key in mta_dict1.keys():\n",
    "        mta_dict1[key].append(value)\n",
    "    else:\n",
    "        mta_dict1[key] = [value]\n",

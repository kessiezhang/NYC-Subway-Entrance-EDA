    "mta_dict_4hrs = {}\n",
    "for key in mta_dict1.keys():\n",
    "    for i, sublist in enumerate(mta_dict1[key]):\n",
    "        if key not in mta_dict_4hrs.keys():\n",
    "            mta_dict_4hrs[key] = [[datetime.strptime('/'.join([sublist[2], sublist[3]]), '%m/%d/%Y/%H:%M:%S'), 0]]\n",
    "        else:\n",
